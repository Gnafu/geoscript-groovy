  <build>
    <plugins>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>2.4.2</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration>
              <shadedArtifactId>geoscript-groovy</shadedArtifactId>
              <finalName>geoscript-groovy-${project.version}</finalName>
              <createDependencyReducedPom>true</createDependencyReducedPom>
              <promoteTransitiveDependencies>true</promoteTransitiveDependencies>
              <transformers>
                <transformer />
                <transformer />
              </transformers>
              <artifactSet>
                <includes>
                  <include>org.geoscript:*</include>
                </includes>
              </artifactSet>
            </configuration>
          </execution>
        </executions>
        <dependencies>
          <dependency>
            <groupId>org.geoscript</groupId>
            <artifactId>geoscript-groovy-maven</artifactId>
            <version>${geoscript.version}</version>
          </dependency>
        </dependencies>
      </plugin>
      <plugin>
        <artifactId>maven-dependency-plugin</artifactId>
        <executions>
          <execution>
            <id>unpack-sources</id>
            <phase>generate-sources</phase>
            <goals>
              <goal>unpack-dependencies</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <classifier>sources</classifier>
          <excludeTransitive>true</excludeTransitive>
          <type>jar</type>
          <outputDirectory>${project.build.directory}/src</outputDirectory>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-antrun-plugin</artifactId>
        <version>1.3</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>run</goal>
            </goals>
            <configuration>
              <tasks>
                <taskdef>
                  <classpath>
                    <path />
                  </classpath>
                </taskdef>
                <Groovydoc />
                <copy />
                <copy />
              </tasks>
            </configuration>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <artifactId>maven-assembly-plugin</artifactId>
        <version>2.4</version>
        <executions>
          <execution>
            <id>make-assembly</id>
            <phase>package</phase>
            <goals>
              <goal>single</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <descriptors>
            <descriptor>dir.xml</descriptor>
            <descriptor>zip.xml</descriptor>
          </descriptors>
        </configuration>
      </plugin>
    </plugins>
  </build>
